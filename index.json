{
	"Componente Menu Submenu Quansar Vue": {
		"prefix": "comp-menu-quasar-vue",
		"body": [
		  "<template>",
		  "    <q-list padding>",
		  "      <q-item",
		  "        v-ripple",
		  "        clickable",
		  "        :to=\"{ name: 'home' }\"",
		  "      >",
		  "        <q-item-section avatar>",
		  "          <q-icon name=\"home\" />",
		  "        </q-item-section>",
		  "        <q-item-section>",
		  "          Home",
		  "        </q-item-section>",
		  "      </q-item>",
		  "      <div",
		  "        v-for=\"(menu, index) in menus\"",
		  "        :key=\"index\"",
		  "      >",
		  "        <q-item",
		  "          v-ripple",
		  "          clickable",
		  "          :to=\"menu.link\"",
		  "          @click=\"menu.exibirSubmenu = !menu.exibirSubmenu\"",
		  "        >",
		  "          <q-item-section avatar>",
		  "            <q-icon :name=\"menu.icone\" />",
		  "          </q-item-section>",
		  "          <q-item-section>",
		  "            {{ menu.nome }}",
		  "          </q-item-section>",
		  "          <q-item-section",
		  "            v-if=\"menu.submenus && menu.submenus.length>0\"",
		  "            side",
		  "          >",
		  "            <q-icon",
		  "              :name=\"menu.exibirSubmenu ? 'expand_less' : 'expand_more'\"",
		  "            />",
		  "          </q-item-section>",
		  "        </q-item>",
		  "        <q-item",
		  "          v-for=\"(submenu, indexFilho) in menu.submenus\"",
		  "          v-show=\"menu.exibirSubmenu\"",
		  "          :key=\"indexFilho\"",
		  "          v-ripple",
		  "          style=\"font-size: small;padding-left: 30px\"",
		  "          clickable",
		  "          :to=\"submenu.link\"",
		  "        >",
		  "          <q-item-section avatar>",
		  "            <q-icon",
		  "              :name=\"submenu.icone\"",
		  "              size=\"large\"",
		  "              style=\"margin: 0\"",
		  "            />",
		  "          </q-item-section>",
		  "          <q-item-section>",
		  "            {{ submenu.nome }}",
		  "          </q-item-section>",
		  "        </q-item>",
		  "      </div>",
		  "      <q-item",
		  "        v-ripple",
		  "        clickable",
		  "        @click=\"logout\"",
		  "      >",
		  "        <q-item-section avatar>",
		  "          <q-icon name=\"exit_to_app\" />",
		  "        </q-item-section>",
		  "        <q-item-section>",
		  "          Sair",
		  "        </q-item-section>",
		  "      </q-item>",
		  "    </q-list>",
		  "  </template>",
		  "  <script>",
		  "  ",
		  "  import axios from 'axios'",
		  "  ",
		  "  export default {",
		  "    name: 'MenuComponent',",
		  "    props: {",
		  "      menus: { // recebe o array de menus e submenus no formato: [{nome: 'Menu 1', link: '/menu1', icone: 'home', submenus: [{nome: 'Submenu 1', link: '/submenu1', icone: 'home'}]}]",
		  "        type: Array,",
		  "        required: true",
		  "      }",
		  "    },",
		  "    data () {",
		  "      return {",
		  "        menusAtivo: false /// para mostrar o sentido da setinha que mostra se o menu está aberto ou fechado",
		  "      }",
		  "    },",
		  "    methods: {",
		  "      logout () { /// ação de logout faz a requisição ao backend para fazer o logout e depois o front redireciona para a tela de login",
		  "        axios.get($1) // rota de logout do backend",
		  "          .then(response => {",
		  "            this.$router.push('/login')",
		  "          })",
		  "      }",
		  "    }",
		  "  }",
		  "  </script>"
		],
		"description": "Componente Menu Submenu Quansar Vue"
	  }
}